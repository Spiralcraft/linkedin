<li:TupleFrame
  xmlns:sax="class:/spiralcraft/data/sax/"
  xmlns:li="class:/spiralcraft/linkedin/api/"
  >
  <elementURI>person
  </elementURI>
  
  <type><li:Person.type/>
  </type>

  <debug>false
  </debug>
  
  <children>
    <sax:ValueFrame>
      <elementURI>id
      </elementURI>
      <assignment>id
      </assignment>
    </sax:ValueFrame>
    
    <sax:ValueFrame>
      <elementURI>first-name
      </elementURI>
      <assignment>firstName
      </assignment>
    </sax:ValueFrame>
    
    <sax:ValueFrame>
      <elementURI>last-name
      </elementURI>
      <assignment>lastName
      </assignment>
    </sax:ValueFrame>
    
    <sax:ValueFrame>
      <elementURI>picture-url
      </elementURI>
      <assignment>pictureURL
      </assignment>
    </sax:ValueFrame>

    <sax:ValueFrame>
      <elementURI>email-address
      </elementURI>
      <assignment>emailAddress
      </assignment>
    </sax:ValueFrame>

  </children>
</li:TupleFrame>